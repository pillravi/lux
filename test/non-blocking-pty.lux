[doc Verify that runpty is non-blocking]

[shell test]
       [invoke 1000]
       [loop i 1..10]
       [invoke 5000]
       [endloop]
#      [timeout 100]
#      [invoke 50000]
       [progress \ndone]

[macro 1000]
[progress \n1000 bytes]
!echo ''
     ?SH-PROMPT
[endmacro]

[macro 5000]
[progress \n5000 bytes]
!echo ''
    ?SH-PROMPT
[endmacro]

[macro 50000]
[progress \n50000 bytes]
#!echo ''
    [timeout 60]
    ?SH-PROMPT
[endmacro]
